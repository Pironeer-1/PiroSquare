<link rel="stylesheet" href="/css/input.css">

<form action="/post/update/<%= post.post_id %>" method="post" enctype="multipart/form-data">
    <input type="file" name="image" value="<%= post.post_image %>">
    <input type="text" name="title" placeholder="title" value="<%= post.title %>" >
    <textarea name="content" id="" cols="20" placeholder="content"><%= post.content %></textarea>
    <button>글 수정 완료</button>
</form>
