<link rel="stylesheet" href="/css/input.css">
<h1>추가정보입력</h1>
<form action="/auth/newUserProfile" method="post" enctype="multipart/form-data">
    <input type="text" name="nickname" placeholder="닉네임">
    <input type="text" name="email" placeholder="이메일">
    <select name="year">
    <% for (let i = 1; i <= 20; i++) { %>
        <option value="<%= i %>">
        <%= i %>기
        </option>
    <% } %>
    </select>
    <input type="file" name="image">
    <textarea name="introduce" id="" cols="20" placeholder="한줄 소개"></textarea>
    <button>추가정보 입력완료</button>
</form>
